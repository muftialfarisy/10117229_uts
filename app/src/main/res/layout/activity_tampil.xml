<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tampil">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="89dp"
        android:gravity="center"
        android:text="Lihat dan Edit Profile"
        android:textSize="24dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/nimj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="85dp"
        android:text="NIM :"/>

    <TextView
        android:id="@+id/namaj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="155dp"
        android:text="Nama :"/>

    <TextView
        android:id="@+id/kelasj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="225dp"
        android:text="Kelas :"/>
    <TextView
        android:id="@+id/telpj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="295dp"
        android:text="telpon :"/>
    <TextView
        android:id="@+id/emailj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="365dp"
        android:text="Email :"/>
    <TextView
        android:id="@+id/sosmedj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="435dp"
        android:text="Instagram :"/>

    <EditText
        android:id="@+id/nimtampil"
        android:layout_width="149dp"
        android:layout_height="58dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="118dp"
        android:layout_marginTop="73dp"
        android:hint="NIM" />
    <EditText
        android:id="@+id/namatampil"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="119dp"
        android:layout_marginTop="144dp"
        android:hint="NAMA"/>
    <EditText
        android:id="@+id/kelastampil"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="119dp"
        android:layout_marginTop="215dp"
        android:hint="KELAS"/>
    <EditText
        android:id="@+id/telptampil"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="119dp"
        android:layout_marginTop="286dp"
        android:hint="TELPON"/>
    <EditText
        android:id="@+id/emailtampil"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="119dp"
        android:layout_marginTop="357dp"
        android:hint="EMAIL"/>
    <EditText
        android:id="@+id/sosmedtampil"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="119dp"
        android:layout_marginTop="428dp"
        android:hint="INSTAGRAM"/>
    <Button
        android:layout_width="107dp"
        android:id="@+id/btn_hapus"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="156dp"
        android:layout_marginTop="531dp"
        android:background="@drawable/btn_rounded2"
        android:text="Delete"
        android:textColor="#FB0303" />
    <Button
        android:layout_width="107dp"
        android:id="@+id/btn_update"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="266dp"
        android:layout_marginTop="531dp"
        android:background="@drawable/btn_rounded"
        android:text="Update"
        android:textColor="#ffff" />
</RelativeLayout>